{
  "manifest_version": 2,

  "name": "Dom Distiller Dev",
  "description": "This extension does various things to support DomDistillerJS development",
  "version": "1.0",

  "icons": {
    "24": "icon24.png",
    "48": "icon48.png",
    "96": "icon96.png"
  },

  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },

  "page_action": {
    "default_title": "Profile Extraction",
    "default_icon": {
      "19": "icon19.png",
      "38": "icon38.png"
    }
  },

  "permissions": [
    "tabs",
    "activeTab"
  ]
}
